<body>
       <h1>Pierre's Bakery</h1>
    @using Bakery2.Models;
    <ul>
        @foreach (string vendorName in Vendor.Lists.Keys)
        {
            <li><a href="/List/@vendorName">@vendorName</a></li>
        }
    </ul>
    <p>Intereste in becoming a vendor of Pierre's fine baked goods?</p>
    <form action="/newVendor" method="get">
        <button>Create a new vendo</button>
    </form>
</body>
</html>